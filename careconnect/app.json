{
  "expo": {
    "name": "careconnect",
    "slug": "CareConnect",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon-main.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#B272A4"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon-main.png",
        "backgroundColor": "#B272A4"
      }
    },
    "web": {
      "favicon": "./assets/app-icon-main.png"
    }
  }
}
